{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "babel-plugin-idx",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      },
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": true
      }
    ],
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "libraryDirectory": "lib",
        "style": "css"
      },
      "mobile"
    ]
  ]
}